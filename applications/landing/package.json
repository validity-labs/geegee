{
  "name": "@validitylabs/geegee-landing",
  "version": "1.0.0",
  "description": "Landingpage for the GeeGee platform",
  "author": "ValidityLabs AG",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "node scripts/env-check.js && next start",
    "start:test": "next start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:next": "tsc --project cypress/next/tsconfig.server.json && NODE_ENV=test node .cypress/cypress/next/server.js",
    "test:e2e": "NODE_ENV=test npm run build && start-server-and-test cypress:next 3000 cypress:open",
    "test:e2e:headless": "NODE_ENV=test npm run build && start-server-and-test cypress:next 3000 cypress:run",
    "test:unit": "TZ=UTC jest",
    "test:unit:update-snap": "TZ=UTC jest --updateSnapshot",
    "test:unit:watch": "TZ=UTC jest --watch",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": "^12.14.1",
    "npm": "^6.13.4"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.9.1",
